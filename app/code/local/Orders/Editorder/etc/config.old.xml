<config>
    <modules>
        <Orders_Editorder>
            <version>0.1.0</version>
        </Orders_Editorder>
    </modules>
    <admin>
        <routers>
            <adminhtml>
                    <use>admin</use>
                    <args>
                        <modules>
                            <foo_bar before="Mage_Adminhtml">Orders_Editorder_Adminhtml</foo_bar>
                        </modules>
                    </args>
            </adminhtml>
        </routers>
    </admin>
    <global>
        <helpers>
            <orders_editorder>
                <class>Mage_Core_Helper</class>
            </orders_editorder>
        </helpers>
        <models>
            <editorder>
                <class>Orders_Editorder_Model</class>
                <resourceModel>editorder_mysql4</resourceModel>
            </editorder>
            <editorder_mysql4>
                <class>Orders_Editorder_Model_Mysql4</class>
                <entities>
                    <order>
                        <table>sales_flat_order</table>
                    </order>
                </entities>
            </editorder_mysql4>
        </models>
        <resources>
            <editorder_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </editorder_write>
            <editorder_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </editorder_read>
        </resources>
    </global>
</config>
