<?php $products = $this->getMatchingProduct(); ?>

<ul>
<?php if($products->count() > 0){ ?>
	<?php foreach ($products as $product){ ?>
		<li  onclick="addProduct(<?php echo $product->getId() ?>, this)">
			<a><?php echo $product->getName(); ?></a>
		</li>
	<?php } ?>

<?php }else{ ?>
	<li><?php echo $this->__("No matching product found for this vendor") ?></li>
<?php } ?>
</ul>