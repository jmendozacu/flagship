/* Header ================================================================================ */
.header-container { width:988px; height:136px; background:url(../images/header.png) no-repeat; margin:0 -4px 0 -4px; }
.header {  }
.header .logo { position:absolute; margin:52px 0 0 24px; text-decoration:none !important; }
.header .logo strong { position:absolute; top:-999em; left:-999em; width:0; height:0; font-size:0; line-height:0; text-indent:-999em; overflow:hidden; }
.header .welcome-msg { position:absolute; margin:19px 0 0 24px; font-weight:bold; color:#6A6969; }
.header .welcome-msg a {}
.header .links { float:right; clear:both; margin:0 0 8px 0; }
.header .links li { padding:0 22px 0 0; }
